<div class="row">
	<div class="col-lg-12">
	   <h2>Highest Number of Enforcement Reports by Manufacturer for the Year 2015</h2>
	</div>
	<div class="row">
		<div class="col-md-6 img-portfolio">
			<div class="contentsbox">
				<table class="boxer">
					<thead  class="box-row">
						<tr>
							<th class="box2">Manufacturers</th>
							<th class="box2">Tota</th>
						</tr>
					</thead>
					<tbody class="box-row1">
					<% @enfEvents.each_with_index do |result, index| %>
						<tr>
							<td class="box"><%= result["term"] %></td>
							<td class="box"><%= result["count"] %></td>
						</tr>
					<% end %>
					</tbody>
				</table>
			</div>
		</div>
		<div class="col-md-6 img-portfolio">
			<div class="contentsbox">
				<h3>Manufacturers Name</h3>
					<%= pie_chart "/enforcements/reports?type=enfbymfr", discrete: true %>
			</div>
		</div>
	</div>
</div> <!-- .row -->